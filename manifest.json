{
  "manifest_version": 2,
  "name": "XERPA",
  "version": "0.1",
  "icons": {
    "16": "./download.png"
  },
  "permissions": [
    "tabs",
    "https://web.whatsapp.com/*",
    "storage"
  ],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "bear"
  }
}
